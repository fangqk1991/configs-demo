{
  "name": "configs-demo",
  "private": true,
  "devDependencies": {
    "prettier": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-standard": "^4.0.1"
  },
  "prettier": {
    "arrowParens": "always",
